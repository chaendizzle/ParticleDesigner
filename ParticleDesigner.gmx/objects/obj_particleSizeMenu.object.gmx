        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// add submenus
event_inherited();

W = getTypeMenuW();
border = 4;

submenus = ds_list_create();

// top bar
addSubmenu(submenus, id, createTextRect(0, 0, W, 30, "Transform", fnt_subtitle, c_black, c_ltgray, c_black, 1, 1, 0));

// scale
