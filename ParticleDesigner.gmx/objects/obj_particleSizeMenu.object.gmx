textrect = createTextRect(64, 32 + 54, 18, 18, "Inc", fnt_kindasmall, c_black, c_white, c_white, 1, 0, 0);
textrect.halign = fa_right;
addSubmenu(submenus, id, textrect);
textrect = createTextRect(64, 32 + 72, 18, 18, "Wig", fnt_kindasmall, c_black, c_white, c_white, 1, 0, 0);
textrect.halign = fa_right;
addSubmenu(submenus, id, textrect);

sizeMin = addSubmenu(submenus, id, createNumBox(64 + 20, 32 + 18, 32, 18, "1", fnt_kindasmall, c_white, c_dkgray, c_black, c_white, 1, 1, 1, 0.5));
sizeMin = addSubmenu(submenus, id, createNumBox(64 + 20, 32 + 36, 32, 18, "1", fnt_kindasmall, c_white, c_dkgray, c_black, c_white, 1, 1, 1, 0.5));
sizeMin = addSubmenu(submenus, id, createNumBox(64 + 20, 32 + 54, 32, 18, "0", fnt_kindasmall, c_white, c_dkgray, c_black, c_white, 1, 1, 1, 0.5));
sizeMin = addSubmenu(submenus, id, createNumBox(64 + 20, 32 + 72, 32, 18, "0", fnt_kindasmall, c_white, c_dkgray, c_black, c_white, 1, 1, 1, 0.5));

// angle
